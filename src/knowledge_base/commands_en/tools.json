    "chmod": {
      "name": "chmod",
      "description": "Change file and directory permissions",
      "category": "Permission Management",
      "usage": "chmod [options] permissions file...",
      "options": [
        {
          "option": "-R",
          "description": "Recursively change permissions of directories and their contents"
        },
        {
          "option": "-v",
          "description": "Display verbose information about permission changes"
        },
        {
          "option": "-c",
          "description": "Only display information when permissions are changed"
        },
        {
          "option": "--reference",
          "description": "Use permissions of another file as reference"
        }
      ],
      "examples": [
        {
          "command": "chmod 755 script.sh",
          "description": "Set script to executable permissions"
        },
        {
          "command": "chmod +x file",
          "description": "Add execute permission"
        },
        {
          "command": "chmod -R 644 directory/",
          "description": "Recursively set file permissions in directory"
        },
        {
          "command": "chmod u+w,g-r,o-r file.txt",
          "description": "Fine-grained permission setting"
        }
