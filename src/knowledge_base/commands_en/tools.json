  "description": "tools commands",
  "commands": {
    "ps": {
      "name": "ps",
      "description": "Display running processes",
      "category": "Process Management",
      "usage": "ps [options]",
      "options": [
        {
          "option": "aux",
          "description": "Show all processes for all users with detailed information"
        },
        {
          "option": "-ef",
          "description": "Show all processes in full format"
        },
        {
          "option": "-u",
          "description": "Show processes for specific user"
        },
        {
          "option": "--forest",
          "description": "Show process tree hierarchy"
        }
      ],
      "examples": [
        {
          "command": "ps aux",
          "description": "Show all running processes"
        },
        {
          "command": "ps -ef | grep python",
          "description": "Find all Python processes"
        },
        {
          "command": "ps -u username",
          "description": "Show processes for specific user"
        },
        {
          "command": "ps aux --forest",
          "description": "Show processes in tree format"
        }
      ],
      "related_commands": [
        "top",
        "htop",
        "kill",
        "jobs"
      ],
      "tags": [
        "processes",
        "system monitoring",
        "running tasks"
      ]
    },
    "pgrep": {
      "command": "pgrep",
      "description": "Find process IDs by name or other criteria",
      "syntax": "pgrep [options] pattern",
      "category": "process_management",
      "options": [
        {
          "option": "-f",
          "description": "Match against full command line, not just process name"
        },
        {
          "option": "-x",
          "description": "Match process name exactly"
        },
        {
          "option": "-u user",
          "description": "Only show processes owned by specified user"
        },
        {
          "option": "-g group",
          "description": "Only show processes belonging to specified process group"
        },
        {
          "option": "-P ppid",
          "description": "Only show child processes of specified parent process"
        },
        {
          "option": "-l",
          "description": "Show process name along with process ID"
        },
        {
          "option": "-a",
          "description": "Show full command line"
        },
        {
          "option": "-n",
          "description": "Show newest matching process"
        },
        {
          "option": "-o",
          "description": "Show oldest matching process"
        }
      ],
      "examples": [
        {
          "command": "pgrep firefox",
          "description": "Find all process IDs for firefox"
        },
        {
          "command": "pgrep -u john",
          "description": "Find all processes owned by user john"
        },
        {
          "command": "pgrep -f python.*test.py",
          "description": "Find processes with command line containing python and test.py"
        },
        {
          "command": "pgrep -l chrome",
          "description": "Find chrome processes and show process names"
        },
        {
          "command": "pgrep -x vim",
          "description": "Find processes with exact name vim"
        },
        {
          "command": "pgrep -n ssh",
          "description": "Find the newest ssh process"
        }
      ],
      "related_commands": [
        "pkill",
        "ps",
        "kill",
        "pidof"
      ],
      "tags": [
        "process",
        "find",
        "system",
        "PID"
      ]
    },
    "kill": {
      "name": "kill",
      "description": "Terminate processes",
      "category": "Process Management",
      "usage": "kill [options] [signal] process_id...",
      "options": [
        {
          "option": "-9",
          "description": "Force terminate process (SIGKILL)"
        },
        {
          "option": "-15",
          "description": "Normal terminate process (SIGTERM, default)"
        },
        {
          "option": "-l",
          "description": "List all available signals"
        },
        {
          "option": "-s",
          "description": "Specify signal to send"
        }
      ],
      "examples": [
        {
          "command": "kill 1234",
          "description": "Normally terminate process 1234"
        },
        {
          "command": "kill -9 1234",
          "description": "Force terminate process 1234"
        },
        {
          "command": "kill -l",
          "description": "Display all signal list"
        },
        {
          "command": "kill -s TERM 1234",
          "description": "Send TERM signal to terminate process"
        }
      ],
      "related_commands": [
        "killall",
        "pkill",
        "ps",
        "top"
      ],
      "tags": [
        "process",
        "terminate",
        "signal",
        "force kill"
      ]
    },
    "killall": {
      "command": "killall",
      "description": "Kill all processes by name",
      "syntax": "killall [options] process_name",
      "category": "process_management",
      "options": [
        {
          "option": "-9",
          "description": "Send SIGKILL signal for forceful termination"
        },
        {
          "option": "-15",
          "description": "Send SIGTERM signal for graceful termination (default)"
        },
        {
          "option": "-u user",
          "description": "Only kill processes owned by specified user"
        },
        {
          "option": "-i",
          "description": "Interactive mode, ask for confirmation before killing"
        },
        {
          "option": "-v",
          "description": "Verbose mode, show operation details"
        },
        {
          "option": "-w",
          "description": "Wait for processes to completely terminate"
        },
        {
          "option": "-q",
          "description": "Quiet mode, don't show error messages"
        },
        {
          "option": "-r",
          "description": "Use regular expressions for process name matching"
        },
        {
          "option": "-o time",
          "description": "Kill processes older than specified time"
        }
      ],
      "examples": [
        {
          "command": "killall firefox",
          "description": "Kill all firefox processes"
        },
        {
          "command": "killall -9 chrome",
          "description": "Forcefully kill all chrome processes"
        },
        {
          "command": "killall -u john vim",
          "description": "Kill all vim processes owned by user john"
        },
        {
          "command": "killall -i python",
          "description": "Interactively kill python processes"
        },
        {
          "command": "killall -v ssh",
          "description": "Kill ssh processes with verbose output"
        },
        {
          "command": "killall -w httpd",
          "description": "Kill httpd and wait for termination"
        }
      ],
      "related_commands": [
        "pkill",
        "kill",
        "pgrep",
        "ps"
      ],
      "tags": [
        "process",
        "kill",
        "system",
        "batch"
      ]
    },
