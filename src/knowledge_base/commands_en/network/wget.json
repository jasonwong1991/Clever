{
  "name": "wget",
  "description": "Download files and web pages",
  "category": "Network Tools",
  "usage": "wget [options] URL",
  "options": [
    {
      "option": "-O",
      "description": "Specify output filename"
    },
    {
      "option": "-c",
      "description": "Continue partial downloads"
    },
    {
      "option": "-r",
      "description": "Recursively download entire website"
    },
    {
      "option": "-q",
      "description": "Quiet mode, no progress display"
    }
  ],
  "examples": [
    {
      "command": "wget https://example.com/file.zip",
      "description": "Download a file"
    },
    {
      "command": "wget -O myfile.pdf https://example.com/document.pdf",
      "description": "Download and rename file"
    },
    {
      "command": "wget -c https://example.com/largefile.zip",
      "description": "Continue interrupted download"
    },
    {
      "command": "wget -r -np https://example.com/directory/",
      "description": "Recursively download directory"
    }
  ],
  "related_commands": ["curl", "scp", "rsync", "ftp"],
  "tags": ["download", "HTTP", "FTP", "network transfer"]
}