{
  "name": "head",
  "description": "Display the beginning of files",
  "category": "File Viewing",
  "usage": "head [options] file...",
  "options": [
    {
      "option": "-n",
      "description": "Specify number of lines to display (default 10)"
    },
    {
      "option": "-c",
      "description": "Display by number of bytes"
    },
    {
      "option": "-q",
      "description": "Do not display file name headers"
    },
    {
      "option": "-v",
      "description": "Always display file name headers"
    }
  ],
  "examples": [
    {
      "command": "head file.txt",
      "description": "Display first 10 lines of file"
    },
    {
      "command": "head -n 20 file.txt",
      "description": "Display first 20 lines of file"
    },
    {
      "command": "head -c 100 file.txt",
      "description": "Display first 100 bytes of file"
    },
    {
      "command": "head -n 5 *.txt",
      "description": "Display first 5 lines of all txt files"
    }
  ],
  "related_commands": ["tail", "cat", "less", "more"],
  "tags": ["file beginning", "partial display", "first lines"]
}