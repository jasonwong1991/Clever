{
  "name": "tail",
  "description": "显示文件末尾部分",
  "category": "文件查看",
  "usage": "tail [选项] 文件...",
  "options": [
    {
      "option": "-n",
      "description": "指定显示的行数（默认10行）"
    },
    {
      "option": "-f",
      "description": "实时监控文件变化"
    },
    {
      "option": "-c",
      "description": "按字节数显示"
    },
    {
      "option": "-F",
      "description": "类似-f，但会重新打开文件"
    }
  ],
  "examples": [
    {
      "command": "tail file.txt",
      "description": "显示文件末尾10行"
    },
    {
      "command": "tail -n 20 file.txt",
      "description": "显示文件末尾20行"
    },
    {
      "command": "tail -f /var/log/messages",
      "description": "实时监控日志文件"
    },
    {
      "command": "tail -c 100 file.txt",
      "description": "显示文件末尾100个字节"
    }
  ],
  "related_commands": ["head", "cat", "less", "more"],
  "tags": ["文件末尾", "实时监控", "日志查看"]
}