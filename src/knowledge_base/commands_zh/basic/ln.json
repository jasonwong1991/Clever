{
  "name": "ln",
  "description": "创建文件或目录的链接",
  "category": "文件管理",
  "usage": "ln [选项] 目标 链接名",
  "options": [
    {
      "option": "-s",
      "description": "创建符号链接（软链接）"
    },
    {
      "option": "-f",
      "description": "强制覆盖已存在的目标文件"
    },
    {
      "option": "-i",
      "description": "覆盖前提示确认"
    },
    {
      "option": "-v",
      "description": "显示创建过程信息"
    }
  ],
  "examples": [
    {
      "command": "ln -s /path/to/file symlink",
      "description": "创建符号链接"
    },
    {
      "command": "ln file.txt hardlink.txt",
      "description": "创建硬链接"
    },
    {
      "command": "ln -sf new_target existing_link",
      "description": "强制更新符号链接"
    },
    {
      "command": "ln -s /usr/bin/python3 python",
      "description": "为python3创建python别名"
    }
  ],
  "related_commands": ["ls", "readlink", "unlink", "stat"],
  "tags": ["链接", "符号链接", "硬链接", "别名"]
}