{
  "command": "gunzip",
  "description": "解压gzip压缩的文件",
  "syntax": "gunzip [选项] [文件...]",
  "category": "compression",
  "options": [
    {
      "option": "-v",
      "description": "显示解压过程信息"
    },
    {
      "option": "-k",
      "description": "保留原压缩文件"
    },
    {
      "option": "-c",
      "description": "输出到标准输出，保留原文件"
    },
    {
      "option": "-t",
      "description": "测试压缩文件完整性"
    },
    {
      "option": "-r",
      "description": "递归解压目录中的所有.gz文件"
    },
    {
      "option": "-f",
      "description": "强制覆盖已存在的文件"
    },
    {
      "option": "-l",
      "description": "列出压缩文件信息"
    }
  ],
  "examples": [
    {
      "command": "gunzip file.txt.gz",
      "description": "解压文件为file.txt"
    },
    {
      "command": "gunzip -v *.gz",
      "description": "解压所有gz文件并显示信息"
    },
    {
      "command": "gunzip -k backup.gz",
      "description": "解压文件但保留原压缩文件"
    },
    {
      "command": "gunzip -c data.gz > output.txt",
      "description": "解压输出到指定文件"
    },
    {
      "command": "gunzip -t archive.gz",
      "description": "测试压缩文件是否完整"
    },
    {
      "command": "gunzip -r /backup/",
      "description": "递归解压目录中所有gz文件"
    }
  ],
  "related_commands": ["gzip", "tar", "zcat", "zless"],
  "tags": ["解压", "gzip", "压缩文件", "解压缩"]
}