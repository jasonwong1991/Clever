    "which": {
      "name": "which",
      "description": "查找命令的位置",
      "category": "系统查询",
      "usage": "which [选项] 命令...",
      "options": [
        {
          "option": "-a",
          "description": "显示PATH中所有匹配的命令"
        },
        {
          "option": "-s",
          "description": "静默模式，不输出任何信息"
        },
        {
          "option": "-v",
          "description": "显示版本信息"
        }
      ],
      "examples": [
        {
          "command": "which python",
          "description": "查找python命令的位置"
        },
        {
          "command": "which -a python",
          "description": "查找所有python命令的位置"
        },
        {
          "command": "which ls cd pwd",
          "description": "查找多个命令的位置"
        }
      ],
      "related_commands": [
        "whereis",
        "type",
        "locate",
        "find"
      ],
      "tags": [
