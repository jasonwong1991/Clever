{
  "category": "tools",
  "category_name": "文件管理",
  "description": "tools commands",
  "commands": {
    "ps": {
      "name": "ps",
      "description": "显示进程状态信息",
      "category": "进程管理",
      "usage": "ps [选项]",
      "options": [
        {
          "option": "aux",
          "description": "显示所有用户的所有进程"
        },
        {
          "option": "-ef",
          "description": "显示所有进程的完整信息"
        },
        {
          "option": "-u",
          "description": "显示指定用户的进程"
        },
        {
          "option": "-p",
          "description": "显示指定PID的进程"
        },
        {
          "option": "-C",
          "description": "显示指定命令名的进程"
        }
      ],
      "examples": [
        {
          "command": "ps aux",
          "description": "显示所有进程的详细信息"
        },
        {
          "command": "ps -ef | grep python",
          "description": "查找包含python的进程"
        },
        {
          "command": "ps -u root",
          "description": "显示root用户的进程"
        },
        {
          "command": "ps -p 1234",
          "description": "显示PID为1234的进程信息"
        }
      ],
      "related_commands": [
        "top",
        "htop",
        "kill",
        "jobs"
      ],
      "tags": [
        "进程",
        "系统",
        "监控",
        "管理"
      ]
    },
    "pgrep": {
      "command": "pgrep",
      "description": "根据进程名称或其他条件查找进程ID",
      "syntax": "pgrep [选项] 模式",
      "category": "process_management",
      "options": [
        {
          "option": "-f",
          "description": "匹配完整的命令行，而不仅仅是进程名"
        },
        {
          "option": "-x",
          "description": "精确匹配进程名"
        },
        {
          "option": "-u 用户",
          "description": "只显示指定用户的进程"
        },
        {
          "option": "-g 组",
          "description": "只显示指定进程组的进程"
        },
        {
          "option": "-P 父进程ID",
          "description": "只显示指定父进程的子进程"
        },
        {
          "option": "-l",
          "description": "显示进程名和进程ID"
        },
        {
          "option": "-a",
          "description": "显示完整的命令行"
        },
        {
          "option": "-n",
          "description": "显示最新的匹配进程"
        },
        {
          "option": "-o",
          "description": "显示最旧的匹配进程"
        }
      ],
      "examples": [
        {
          "command": "pgrep firefox",
          "description": "查找所有名为firefox的进程ID"
        },
        {
          "command": "pgrep -u john",
          "description": "查找用户john的所有进程"
        },
        {
          "command": "pgrep -f python.*test.py",
          "description": "查找命令行包含python和test.py的进程"
        },
        {
          "command": "pgrep -l chrome",
          "description": "查找chrome进程并显示进程名"
        },
        {
          "command": "pgrep -x vim",
          "description": "精确匹配名为vim的进程"
        },
        {
          "command": "pgrep -n ssh",
          "description": "查找最新的ssh进程"
        }
      ],
      "related_commands": [
        "pkill",
        "ps",
        "kill",
        "pidof"
      ],
      "tags": [
        "进程",
        "查找",
        "系统管理",
        "进程ID"
      ]
    },
    "kill": {
      "name": "kill",
      "description": "终止进程",
      "category": "进程管理",
      "usage": "kill [选项] [信号] 进程ID...",
      "options": [
        {
          "option": "-9",
          "description": "强制终止进程（SIGKILL）"
        },
        {
          "option": "-15",
          "description": "正常终止进程（SIGTERM，默认）"
        },
        {
          "option": "-l",
          "description": "列出所有可用信号"
        },
        {
          "option": "-s",
          "description": "指定发送的信号"
        }
      ],
      "examples": [
        {
          "command": "kill 1234",
          "description": "正常终止进程1234"
        },
        {
          "command": "kill -9 1234",
          "description": "强制终止进程1234"
        },
        {
          "command": "kill -l",
          "description": "显示所有信号列表"
        },
        {
          "command": "kill -s TERM 1234",
          "description": "发送TERM信号终止进程"
        }
      ],
      "related_commands": [
        "killall",
        "pkill",
        "ps",
        "top"
      ],
      "tags": [
        "进程",
        "终止",
        "信号",
        "强制终止"
      ]
    },
    "killall": {
      "command": "killall",
      "description": "根据进程名称终止所有匹配的进程",
      "syntax": "killall [选项] 进程名",
      "category": "process_management",
      "options": [
        {
          "option": "-9",
          "description": "发送SIGKILL信号强制终止"
        },
        {
          "option": "-15",
          "description": "发送SIGTERM信号正常终止(默认)"
        },
        {
          "option": "-u 用户",
          "description": "只终止指定用户的进程"
        },
        {
          "option": "-i",
          "description": "交互模式，终止前询问确认"
        },
        {
          "option": "-v",
          "description": "详细模式，显示操作信息"
        },
        {
          "option": "-w",
          "description": "等待进程完全终止"
        },
        {
          "option": "-q",
          "description": "安静模式，不显示错误信息"
        },
        {
          "option": "-r",
          "description": "使用正则表达式匹配进程名"
        },
        {
          "option": "-o 时间",
          "description": "终止运行超过指定时间的进程"
        }
      ],
      "examples": [
        {
          "command": "killall firefox",
          "description": "终止所有firefox进程"
        },
        {
          "command": "killall -9 chrome",
          "description": "强制终止所有chrome进程"
        },
        {
          "command": "killall -u john vim",
          "description": "终止用户john的所有vim进程"
        },
        {
          "command": "killall -i python",
          "description": "交互式终止python进程"
        },
        {
          "command": "killall -v ssh",
          "description": "详细显示ssh进程终止信息"
        },
        {
          "command": "killall -w httpd",
          "description": "终止httpd并等待完成"
        }
      ],
      "related_commands": [
        "pkill",
        "kill",
        "pgrep",
        "ps"
      ],
      "tags": [
        "进程",
        "终止",
        "系统管理",
        "批量操作"
      ]
    },
