    "lsof": {
      "command": "lsof",
      "description": "显示系统中被进程打开的文件列表",
      "syntax": "lsof [选项] [文件名|进程]",
      "category": "system_info",
      "options": [
        {
          "option": "-p PID",
          "description": "显示指定进程ID打开的文件"
        },
        {
          "option": "-u 用户",
          "description": "显示指定用户打开的文件"
        },
        {
          "option": "-c 进程名",
          "description": "显示指定进程名打开的文件"
        },
        {
          "option": "-i [协议][:端口]",
          "description": "显示网络连接相关的文件"
        },
        {
          "option": "-t",
          "description": "仅显示进程ID"
        },
        {
          "option": "-n",
          "description": "显示IP地址而不是主机名"
        },
        {
          "option": "-P",
          "description": "显示端口号而不是服务名"
        },
        {
          "option": "+D 目录",
          "description": "显示指定目录及其子目录中被打开的文件"
        },
        {
          "option": "-a",
          "description": "AND逻辑，所有条件必须满足"
        }
      ],
      "examples": [
        {
          "command": "lsof /var/log/messages",
          "description": "查看哪个进程在使用该文件"
        },
        {
          "command": "lsof -u root",
          "description": "显示root用户打开的所有文件"
        },
        {
          "command": "lsof -c ssh",
          "description": "显示ssh进程打开的文件"
        },
        {
          "command": "lsof -i :22",
          "description": "显示使用22端口的进程"
        },
        {
          "command": "lsof -i tcp:80",
          "description": "显示使用TCP 80端口的进程"
        },
        {
          "command": "lsof -p 1234",
          "description": "显示进程ID为1234的进程打开的文件"
        },
        {
          "command": "lsof +D /tmp",
          "description": "显示/tmp目录下被打开的文件"
        }
      ],
      "related_commands": [
        "ps",
        "netstat",
        "ss",
        "fuser"
      ],
      "tags": [
        "文件",
        "进程",
        "网络连接",
        "系统监控",
        "调试"
      ]
    },
