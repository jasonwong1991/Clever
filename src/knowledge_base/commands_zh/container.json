  "commands": {
    "kubectl": {
      "command": "kubectl",
      "description": "Kubernetes集群管理工具",
      "syntax": "kubectl [命令] [类型] [名称] [选项]",
      "category": "containerization",
      "options": [
        {
          "option": "get",
          "description": "显示资源列表"
        },
        {
          "option": "describe",
          "description": "显示资源详细信息"
        },
        {
          "option": "create",
          "description": "创建资源"
        },
        {
          "option": "delete",
          "description": "删除资源"
        },
        {
          "option": "apply",
          "description": "应用配置文件"
        },
        {
          "option": "logs",
          "description": "查看Pod日志"
        },
        {
          "option": "exec",
          "description": "在Pod中执行命令"
        },
        {
          "option": "-n 命名空间",
          "description": "指定命名空间"
        },
        {
          "option": "-o 格式",
          "description": "指定输出格式(yaml, json等)"
        }
      ],
      "examples": [
        {
          "command": "kubectl get pods",
          "description": "列出所有Pod"
        },
        {
          "command": "kubectl get services -n kube-system",
          "description": "列出kube-system命名空间的服务"
        },
        {
          "command": "kubectl describe pod my-pod",
          "description": "查看Pod详细信息"
        },
        {
          "command": "kubectl logs my-pod",
          "description": "查看Pod日志"
        },
        {
          "command": "kubectl exec -it my-pod -- /bin/bash",
          "description": "进入Pod执行命令"
        },
        {
          "command": "kubectl apply -f deployment.yaml",
          "description": "应用配置文件"
        },
        {
          "command": "kubectl delete pod my-pod",
          "description": "删除Pod"
        }
      ],
      "related_commands": [
        "docker",
        "helm",
        "minikube",
        "kubeadm"
      ],
      "tags": [
        "Kubernetes",
        "容器编排",
        "集群管理",
        "Pod",
        "微服务"
      ]
    },
    "docker-compose": {
      "command": "docker-compose",
      "description": "Docker多容器应用编排工具",
      "syntax": "docker-compose [选项] [命令] [参数]",
      "category": "containerization",
      "options": [
        {
          "option": "up",
          "description": "创建并启动容器"
        },
        {
          "option": "down",
          "description": "停止并删除容器、网络"
        },
        {
          "option": "build",
          "description": "构建服务镜像"
        },
        {
          "option": "ps",
          "description": "列出容器"
        },
        {
          "option": "logs",
          "description": "查看服务日志"
        },
        {
          "option": "exec",
          "description": "在运行的容器中执行命令"
        },
        {
          "option": "-f 文件",
          "description": "指定compose文件"
        },
        {
          "option": "-d",
          "description": "后台运行"
        },
        {
          "option": "--scale 服务=数量",
          "description": "扩展服务实例数"
        }
      ],
      "examples": [
        {
          "command": "docker-compose up",
          "description": "启动所有服务"
        },
        {
          "command": "docker-compose up -d",
          "description": "后台启动所有服务"
        },
        {
          "command": "docker-compose down",
          "description": "停止并删除所有容器"
        },
        {
          "command": "docker-compose build",
          "description": "构建所有服务镜像"
        },
        {
          "command": "docker-compose logs web",
          "description": "查看web服务日志"
        },
        {
          "command": "docker-compose exec web bash",
          "description": "进入web容器执行bash"
        },
        {
          "command": "docker-compose up --scale web=3",
          "description": "启动3个web服务实例"
        }
      ],
      "related_commands": [
        "docker",
        "kubectl",
