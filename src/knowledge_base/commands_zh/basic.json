      "usage": "find [路径] [表达式]",
      "options": [
        {
          "option": "-name",
          "description": "按文件名查找"
        },
        {
          "option": "-type",
          "description": "按文件类型查找（f=文件, d=目录）"
        },
        {
          "option": "-size",
          "description": "按文件大小查找"
        },
        {
          "option": "-exec",
          "description": "对找到的文件执行命令"
        }
