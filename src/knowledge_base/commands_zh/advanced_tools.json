    "rsync": {
      "command": "rsync",
      "description": "高效的文件同步和传输工具",
      "syntax": "rsync [选项] 源 目标",
      "category": "network_tools",
      "options": [
        {
          "option": "-a",
          "description": "归档模式，保持文件属性(-rlptgoD)"
        },
        {
          "option": "-v",
          "description": "详细输出，显示传输的文件"
        },
        {
          "option": "-z",
          "description": "传输时压缩数据"
        },
        {
          "option": "-r",
          "description": "递归复制目录"
        },
        {
          "option": "-u",
          "description": "只更新较新的文件"
        },
        {
          "option": "--delete",
          "description": "删除目标中源没有的文件"
        },
        {
          "option": "--exclude=模式",
          "description": "排除匹配模式的文件"
        },
        {
          "option": "--progress",
          "description": "显示传输进度"
        },
        {
          "option": "-n",
          "description": "试运行，不实际传输"
        },
        {
          "option": "-e ssh",
          "description": "通过SSH传输"
        }
      ],
      "examples": [
        {
          "command": "rsync -av source/ dest/",
          "description": "同步目录，保持属性"
        },
        {
          "command": "rsync -avz user@host:/path/ /local/",
          "description": "从远程同步并压缩传输"
        },
        {
          "command": "rsync -av --delete source/ dest/",
          "description": "同步并删除多余文件"
        },
        {
          "command": "rsync -av --exclude='*.log' project/ backup/",
          "description": "同步项目但排除日志文件"
        },
        {
          "command": "rsync -av --progress large_file remote:/path/",
          "description": "传输大文件并显示进度"
        },
        {
          "command": "rsync -avn source/ dest/",
          "description": "试运行，查看将要同步的文件"
        }
      ],
      "related_commands": [
        "scp",
        "cp",
        "ssh",
        "tar"
      ],
      "tags": [
        "文件同步",
        "远程传输",
        "备份",
        "增量同步",
        "网络工具"
      ]
    },
    "xargs": {
      "command": "xargs",
      "description": "从标准输入构建和执行命令行",
      "syntax": "xargs [选项] [命令]",
      "category": "advanced_tools",
      "options": [
        {
          "option": "-I 替换字符串",
          "description": "指定替换字符串"
        },
        {
          "option": "-n 数量",
          "description": "每次最多使用指定数量的参数"
        },
        {
          "option": "-d 分隔符",
          "description": "指定输入分隔符"
        },
        {
          "option": "-p",
          "description": "执行前询问确认"
        },
        {
          "option": "-t",
          "description": "执行前显示命令"
        },
        {
          "option": "-0",
          "description": "输入以null字符分隔"
        },
        {
          "option": "-P 进程数",
          "description": "并行运行指定数量的进程"
        },
        {
          "option": "-r",
          "description": "如果没有输入则不运行命令"
        }
      ],
      "examples": [
        {
          "command": "find . -name '*.txt' | xargs rm",
          "description": "删除所有txt文件"
        },
        {
          "command": "echo 'file1 file2 file3' | xargs -n 1 ls -l",
          "description": "对每个文件单独执行ls -l"
        },
        {
          "command": "find . -name '*.log' | xargs -I {} cp {} /backup/",
          "description": "复制所有log文件到备份目录"
        },
        {
          "command": "ls *.jpg | xargs -n 1 -P 4 convert {} {}.png",
          "description": "并行转换图片格式"
        },
        {
          "command": "find . -name '*.c' -print0 | xargs -0 grep 'main'",
          "description": "在C文件中搜索main函数"
        },
        {
          "command": "echo 'dir1 dir2 dir3' | xargs mkdir",
          "description": "批量创建目录"
        }
      ],
      "related_commands": [
        "find",
        "grep",
        "awk",
        "parallel"
      ],
