{
  "name": "ln",
  "description": "Create links between files or directories",
  "category": "File Management",
  "usage": "ln [options] target link_name",
  "options": [
    {
      "option": "-s",
      "description": "Create symbolic link (soft link)"
    },
    {
      "option": "-f",
      "description": "Force overwrite existing target file"
    },
    {
      "option": "-i",
      "description": "Prompt before overwriting"
    },
    {
      "option": "-v",
      "description": "Display verbose information about creation process"
    }
  ],
  "examples": [
    {
      "command": "ln -s /path/to/file symlink",
      "description": "Create a symbolic link"
    },
    {
      "command": "ln file.txt hardlink.txt",
      "description": "Create a hard link"
    },
    {
      "command": "ln -sf new_target existing_link",
      "description": "Force update symbolic link"
    },
    {
      "command": "ln -s /usr/bin/python3 python",
      "description": "Create python alias for python3"
    }
  ],
  "related_commands": ["ls", "readlink", "unlink", "stat"],
  "tags": ["link", "symbolic link", "hard link", "alias"]
}