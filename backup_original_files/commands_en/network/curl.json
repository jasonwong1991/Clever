{
  "name": "curl",
  "description": "Transfer data to or from servers",
  "category": "Network Tools",
  "usage": "curl [options] URL",
  "options": [
    {
      "option": "-o",
      "description": "Write output to file"
    },
    {
      "option": "-X",
      "description": "Specify HTTP request method"
    },
    {
      "option": "-H",
      "description": "Add HTTP header"
    },
    {
      "option": "-d",
      "description": "Send POST data"
    }
  ],
  "examples": [
    {
      "command": "curl https://api.example.com",
      "description": "Get API data"
    },
    {
      "command": "curl -o file.html https://example.com",
      "description": "Download webpage to file"
    },
    {
      "command": "curl -X POST -d 'data=value' https://api.example.com",
      "description": "Send POST request"
    },
    {
      "command": "curl -H 'Content-Type: application/json' https://api.example.com",
      "description": "Request with custom headers"
    }
  ],
  "related_commands": ["wget", "http", "httpie", "lynx"],
  "tags": ["HTTP", "API", "data transfer", "REST"]
}