{
  "name": "netstat",
  "description": "显示网络连接、路由表和网络统计",
  "category": "网络监控",
  "usage": "netstat [选项]",
  "options": [
    {
      "option": "-a",
      "description": "显示所有连接和监听端口"
    },
    {
      "option": "-n",
      "description": "以数字形式显示地址和端口"
    },
    {
      "option": "-t",
      "description": "仅显示TCP连接"
    },
    {
      "option": "-l",
      "description": "仅显示监听端口"
    }
  ],
  "examples": [
    {
      "command": "netstat -an",
      "description": "显示所有网络连接"
    },
    {
      "command": "netstat -tlnp",
      "description": "显示TCP监听端口和进程"
    },
    {
      "command": "netstat -rn",
      "description": "显示路由表"
    },
    {
      "command": "netstat -i",
      "description": "显示网络接口统计"
    }
  ],
  "related_commands": ["ss", "lsof", "nmap", "iptables"],
  "tags": ["网络连接", "端口", "路由", "网络监控"]
}