{
  "name": "rmdir",
  "description": "删除空目录",
  "category": "文件管理",
  "usage": "rmdir [选项] 目录名...",
  "options": [
    {
      "option": "-p",
      "description": "递归删除空目录和其父目录"
    },
    {
      "option": "-v",
      "description": "显示删除过程信息"
    },
    {
      "option": "--ignore-fail-on-non-empty",
      "description": "忽略非空目录的错误"
    }
  ],
  "examples": [
    {
      "command": "rmdir emptydir",
      "description": "删除空目录"
    },
    {
      "command": "rmdir -p parent/child",
      "description": "递归删除空目录和父目录"
    },
    {
      "command": "rmdir -v dir1 dir2",
      "description": "删除多个空目录并显示过程"
    }
  ],
  "related_commands": ["mkdir", "rm", "ls"],
  "tags": ["目录", "删除", "空目录"]
}