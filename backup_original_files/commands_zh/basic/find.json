{
  "name": "find",
  "description": "在目录树中查找文件和目录",
  "category": "文件查找",
  "usage": "find [路径] [表达式]",
  "options": [
    {
      "option": "-name",
      "description": "按文件名查找"
    },
    {
      "option": "-type",
      "description": "按文件类型查找（f=文件, d=目录）"
    },
    {
      "option": "-size",
      "description": "按文件大小查找"
    },
    {
      "option": "-exec",
      "description": "对找到的文件执行命令"
    }
  ],
  "examples": [
    {
      "command": "find . -name '*.txt'",
      "description": "查找当前目录下所有txt文件"
    },
    {
      "command": "find /home -type d -name 'backup'",
      "description": "查找名为backup的目录"
    },
    {
      "command": "find . -size +10M",
      "description": "查找大于10MB的文件"
    },
    {
      "command": "find . -name '*.log' -exec rm {} \\;",
      "description": "查找并删除所有log文件"
    }
  ],
  "related_commands": ["locate", "which", "whereis", "grep"],
  "tags": ["查找", "文件", "搜索", "目录"]
}