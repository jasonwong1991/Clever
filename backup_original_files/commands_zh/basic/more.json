{
  "name": "more",
  "description": "分页显示文件内容（简单版）",
  "category": "文件查看",
  "usage": "more [选项] 文件...",
  "options": [
    {
      "option": "-d",
      "description": "显示用户友好的提示信息"
    },
    {
      "option": "-f",
      "description": "不折行显示长行"
    },
    {
      "option": "-p",
      "description": "不滚动，清屏显示"
    },
    {
      "option": "-s",
      "description": "压缩多个空行为一行"
    }
  ],
  "examples": [
    {
      "command": "more file.txt",
      "description": "分页显示文件内容"
    },
    {
      "command": "more -d longfile.txt",
      "description": "带提示信息显示文件"
    },
    {
      "command": "ls -la | more",
      "description": "分页显示命令输出"
    }
  ],
  "related_commands": ["less", "cat", "head", "tail"],
  "tags": ["分页显示", "文件查看", "简单分页"]
}