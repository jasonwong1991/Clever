{
  "name": "mv",
  "description": "移动或重命名文件和目录",
  "category": "文件管理",
  "usage": "mv [选项] 源文件 目标文件",
  "options": [
    {
      "option": "-i",
      "description": "覆盖前提示确认"
    },
    {
      "option": "-f",
      "description": "强制覆盖，不提示"
    },
    {
      "option": "-v",
      "description": "显示移动过程信息"
    },
    {
      "option": "-n",
      "description": "不覆盖已存在的文件"
    }
  ],
  "examples": [
    {
      "command": "mv oldname.txt newname.txt",
      "description": "重命名文件"
    },
    {
      "command": "mv file.txt /path/to/destination/",
      "description": "移动文件到指定目录"
    },
    {
      "command": "mv -i *.txt backup/",
      "description": "移动所有txt文件并提示确认"
    },
    {
      "command": "mv directory1 directory2",
      "description": "重命名目录"
    }
  ],
  "related_commands": ["cp", "rm", "rename", "ln"],
  "tags": ["移动", "重命名", "文件", "目录"]
}