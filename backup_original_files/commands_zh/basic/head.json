{
  "name": "head",
  "description": "显示文件开头部分",
  "category": "文件查看",
  "usage": "head [选项] 文件...",
  "options": [
    {
      "option": "-n",
      "description": "指定显示的行数（默认10行）"
    },
    {
      "option": "-c",
      "description": "按字节数显示"
    },
    {
      "option": "-q",
      "description": "不显示文件名头部"
    },
    {
      "option": "-v",
      "description": "总是显示文件名头部"
    }
  ],
  "examples": [
    {
      "command": "head file.txt",
      "description": "显示文件前10行"
    },
    {
      "command": "head -n 20 file.txt",
      "description": "显示文件前20行"
    },
    {
      "command": "head -c 100 file.txt",
      "description": "显示文件前100个字节"
    },
    {
      "command": "head -n 5 *.txt",
      "description": "显示所有txt文件的前5行"
    }
  ],
  "related_commands": ["tail", "cat", "less", "more"],
  "tags": ["文件开头", "部分显示", "前几行"]
}