{
  "name": "jobs",
  "description": "显示当前终端的作业列表",
  "category": "进程管理",
  "usage": "jobs [选项]",
  "options": [
    {
      "option": "-l",
      "description": "显示进程ID和详细信息"
    },
    {
      "option": "-p",
      "description": "仅显示进程ID"
    },
    {
      "option": "-r",
      "description": "仅显示运行中的作业"
    },
    {
      "option": "-s",
      "description": "仅显示停止的作业"
    }
  ],
  "examples": [
    {
      "command": "jobs",
      "description": "显示所有作业"
    },
    {
      "command": "jobs -l",
      "description": "显示作业及其进程ID"
    },
    {
      "command": "jobs -r",
      "description": "显示运行中的作业"
    },
    {
      "command": "jobs %1",
      "description": "显示指定作业信息"
    }
  ],
  "related_commands": ["bg", "fg", "nohup", "ps"],
  "tags": ["作业", "后台进程", "进程管理"]
}