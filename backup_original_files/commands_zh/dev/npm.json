{
  "name": "npm",
  "description": "Node.js包管理器",
  "category": "开发工具",
  "usage": "npm [命令] [选项]",
  "options": [
    {
      "option": "install",
      "description": "安装依赖包"
    },
    {
      "option": "start",
      "description": "运行start脚本"
    },
    {
      "option": "test",
      "description": "运行测试脚本"
    },
    {
      "option": "build",
      "description": "运行构建脚本"
    },
    {
      "option": "init",
      "description": "初始化package.json"
    },
    {
      "option": "publish",
      "description": "发布包到npm仓库"
    },
    {
      "option": "update",
      "description": "更新依赖包"
    },
    {
      "option": "list",
      "description": "列出已安装的包"
    }
  ],
  "examples": [
    {
      "command": "npm install",
      "description": "安装package.json中的所有依赖"
    },
    {
      "command": "npm install express",
      "description": "安装express包"
    },
    {
      "command": "npm start",
      "description": "运行项目"
    },
    {
      "command": "npm run build",
      "description": "构建项目"
    }
  ],
  "related_commands": ["yarn", "node", "npx"],
  "tags": ["Node.js", "包管理", "JavaScript", "开发"]
}